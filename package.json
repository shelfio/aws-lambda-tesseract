{
  "name": "@shelf/aws-lambda-tesseract",
  "version": "3.0.0",
  "description": "6 MB Tesseract 4.1 (with English training data) to fit inside AWS Lambda",
  "license": "MIT",
  "repository": "shelfio/aws-lambda-tesseract",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "test": "TZ=UTC jest src",
    "coverage": "jest --coverage",
    "prepack": "yarn build",
    "lint": "eslint . --ext .js,.ts,.json --fix --quiet",
    "lint:ci": "eslint . --ext .js,.ts,.json --quiet",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "rm -rf lib/ && yarn build:types && babel src --out-dir lib --ignore '**/*.test.ts' --extensions '.ts'",
    "build:types": "tsc --emitDeclarationOnly --declaration --isolatedModules false --declarationDir lib"
  },
  "files": [
    "lib",
    "bin/tt.tar.gz"
  ],
  "main": "lib",
  "types": "lib/index.d.ts",
  "keywords": [
    "aws",
    "lambda",
    "ocr",
    "tesseract"
  ],
  "dependencies": {
    "is-image": "3.0.0",
    "tar": "6.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.1",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-typescript": "7.10.4",
    "@shelf/eslint-config": "0.20.0",
    "@types/jest": "26.0.9",
    "@types/node": "12",
    "@types/tar": "4.0.3",
    "eslint": "7.6.0",
    "husky": "4.2.5",
    "jest": "26.2.2",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "typescript": "3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.{html,json,md,yml}": [
      "prettier --write --ignore-path=./.eslintignore",
      "git add"
    ],
    "*.{ts,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
