{
  "name": "@shelf/aws-lambda-tesseract",
  "version": "1.3.2",
  "description": "11 MB Tesseract (with English training data) to fit inside AWS Lambda compressed with Brotli",
  "license": "MIT",
  "repository": "shelfio/aws-lambda-tesseract",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "engines": {
    "node": ">=8.10"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "test": "TZ=UTC jest src"
  },
  "files": [
    "src",
    "bin"
  ],
  "main": "src/index.js",
  "keywords": [
    "lambda",
    "ocr",
    "tesseract",
    "brotli"
  ],
  "dependencies": {
    "@shelf/aws-lambda-brotli-unpacker": "0.0.2",
    "is-image": "2.0.0"
  },
  "devDependencies": {
    "eslint": "5.12.1",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-prettier": "3.0.1",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "prettier": "1.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
